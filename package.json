{
  "name": "noverterm",
  "private": true,
  "version": "0.1.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "tauri": "tauri",
    "tauri:dev": "tauri dev",
    "tauri:build": "tauri build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --check .",
    "format:fix": "prettier --write .",
    "typecheck": "tsc --noEmit",
    "check:rust": "cd src-tauri && cargo clippy --all-targets --all-features",
    "check:rust:fix": "cd src-tauri && cargo clippy --all-targets --all-features --fix --allow-dirty",
    "fmt:rust": "cd src-tauri && cargo fmt",
    "check:all": "npm run typecheck && npm run lint && npm run check:rust"
  },
